{
  "SeeDocsAt": "https://github.com/Microsoft/AirSim/blob/master/docs/settings_json.md",
  "SettingsVersion": 1.2,
  "SimMode": "Car",
  "ClockSpeed": 5,
  "SpeedUnitFactor": 1,
  "SpeedUnitLabel": "m/s",
  "ApiServerPort": 41451,
